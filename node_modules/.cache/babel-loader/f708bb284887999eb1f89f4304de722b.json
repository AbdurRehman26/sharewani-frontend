opup\"\n            :title=\"title\"\n            size=\"sm\"\n            :okTitle=\"buttonText\"\n            cancelTitle=\"Cancel\"\n            cancelVariant=\"link\"\n        >\n            <div id=\"style-2\">\n                <b-form>\n                    <div class=\"row\">\n                        <div class=\"col-md-12\">\n                            <b-form-group\n                                id=\"input-group-1\"\n                                label=\"Label\"\n                                label-for=\"field-label\"\n                            >\n                                <b-form-input\n                                    id=\"field-label\"\n                                    type=\"text\"\n                                    v-model=\"form.fieldLabel\"\n                                    required\n                                    autocomplete=\"no\"\n                                    placeholder=\"Enter field label\"\n                                ></b-form-input>\n                            </b-form-group>\n                        </div>\n                        <div class=\"col-md-12 row-min-space\">\n                            <div class=\"scrollable more-fields-popup\">\n                                <div class=\"row\">\n                                    <div class=\"col-md-6\">\n                                        <label class=\"m-b-15\" for=\"options\"\n                                            >OPTIONS</label\n                                        >\n                                    </div>\n                                    <div class=\"col-md-6\">\n                                        <label class=\"m-b-15\" for=\"values\"\n                                            >VALUE</label\n                                        >\n                                    </div>\n                                </div>\n                                <div\n                                    class=\"row\"\n         