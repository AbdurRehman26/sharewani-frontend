  </div>\n        </b-modal>\n    </div>\n</template>\n\n<script>\nexport default {\n    /*\n    |--------------------------------------------------------------------------\n    | Component > props\n    |--------------------------------------------------------------------------\n    */\n    props: {\n        /**\n         * Value to determine the current compose mode which\n         * varies between 'add' and 'edit'\n         */\n        title: {\n            type: String,\n            default: 'Add Text Field',\n        },\n        buttonText: {\n            type: String,\n            default: 'Create',\n        },\n    }, // End of Component > props\n\n    /*\n    |--------------------------------------------------------------------------\n    | Component > data\n    |--------------------------------------------------------------------------\n    */\n\n    data() {\n        return {\n            form: {\n                fieldLabel: '',\n                options: [\n                    {\n                        optionName: '',\n                        optionVal: null,\n                    },\n                    {\n                        optionName: '',\n                        optionVal: null,\n                    },\n                ],\n                isrequiredModel: null,\n                isrequired: [{ text: 'Yes', value: null }, 'No'],\n            },\n        }\n    },\n    methods: {\n        /**\n         * Handle when the modal is canceled\n         *\n         * @return {void}\n         */\n        addOption() {\n            this.form.options.push({ optionName: '', optionVal: null })\n        },\n        deleteRow(index) {\n            this.form.options.splice(index, 1)\n        },\n    },\n}\n</script>\n"],"sourceRoot":"src/components/popups/form-builder"}]}